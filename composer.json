{
  "name": "eom/plugin-cakephp-loadsubcomponents",
  "type": "cakephp-plugin",
  "description": "Plugin CakePHP 2.x for Load SubComponents in SubFolders.",
  "homepage": "https://github.com/EOM/plugin-cakephp-loadsubcomponents",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Sergio de EOM Design Group",
      "homepage": "https://eom.com.ar/#PluginCakePHPLoadSubComponents",
      "role": "CEO and Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "cakephp/cakephp": ">=2.10",
    "composer/installers": "*"
  },
  "extra": {
    "installer-name": "LoadSubComponents",
    "installer-paths": {
      "app/Plugin/EOM/{$name}/": ["vendor:eom","type:cakephp-plugin"]
    }
  }
}
